0,80], histtype='step', label='Bottom PMT', color='b')\n","plt.xlabel('Baselines (mV)')\n","plt.legend(); \n","plt.grid(); \n","\n","plt.figure(figsize=(14,4))\n","plt.hist(bl_stddev_0, bins=200, range=[0,10], histtype='step', label='Top PMT', color='r')\n","plt.hist(bl_stddev_1, bins=200, range=[0,10], histtype='step', label='Bottom PMT', color='b')\n","plt.xlabel('Baseline Standard Deviation (mV)')\n","plt.legend(); \n","plt.grid();\n","\n","plt.figure(figsize=(12,4))\n","plt.hist2d(timestamp_min[c_good & c_Xe131m], area_0a[c_good & c_Xe131m],\\\n","         range=[[lwrlim,uprlim],[0,110000]], bins=[500,100],norm=LogNorm())\n","plt.xlabel('Minutes since '+files[0])\n","plt.ylabel('Top PMT Pulse Area with Xe131m Selection (mV*10ns)')\n","plt.grid(); \n","\n","plt.figure(figsize=(12,4))\n","plt.hist2d(timestamp_min[c_good & c_Xe131m], area_1a[c_good & c_Xe131m],\\\n","         range=[[lwrlim,uprlim],[0,700000]], bins=[500,100],norm=LogNorm())\n","plt.xlabel('Minutes since '+files[0])\n","plt.ylabel('Bottom PMT Pulse Area with Xe131m Selection (mV*10ns)')\n","plt.grid(); \n","\n","plt.show()"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"peQkaVyYHd2-"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"R_VBnJsgHd2_"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"J-QVs2ctHd2_"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"0veyTVTDHd3A"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"ureEFguzHd3B"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"scrolled":true,"id":"XAevY5EFHd3B"},"source":["# Single Exponential, two ways\n","\n","start = 1500.0 # Set the number of minutes after the peak value to begin the fit\n","fit_range = 20000.0 # Set the number of minutes to be fitted\n","bin_width = 180.0 # bin width in seconds\n","\n","xe_dc = np.log(2)/17049.6 #minutes\n","\n","def fit1(bin_centers, data):\n","    import numpy as np\n","    from scipy.optimize import curve_fit\n","    popt, pcov = curve_fit(lambda t, A: (A*np.exp(-t*xe_dc)), \\\n","                           bin_centers, data, p0=[1], bounds=([0], np.inf))                                  \n","    A     = popt[0]; sig_A = np.sqrt(pcov[0,0])\n","    return (A, sig_A)\n","\n","def fit2(bin_centers, data):\n","    import numpy as np\n","    from scipy.optimize import curve_fit\n","    popt, pcov = curve_fit(lambda t, B, tau: (B*np.exp(-t/tau)), \\\n","                           bin_centers, data, p0=[1, 17050], bounds=([0, 0], np.inf))                                  \n","    B = popt[0]; tau = popt[1];\n","    sig_B = np.sqrt(pcov[0,0]); sig_tau = np.sqrt(pcov[1,1]);\n","    return (B, sig_B, tau, sig_tau)\n","\n","w1 = np.zeros(len(timestamp[c_good])) \n","w2 = np.zeros(len(timestamp[c_good & c_Xe131m])) \n","w1.fill(1.0/bin_width); w2.fill(1.0/bin_width)\n","\n","plt.figure(figsize=(13,4))\n","y, x_tmp, _ = plt.hist(timestamp_min[c_Xe131m & c_good], \\\n","                       bins=int(((uprlim-lwrlim)*60.0)/bin_width),\\\n","                       range=[lwrlim, uprlim], weights=w2, histtype='step',color='orange')\n","\n","# low = bin_number_of_injection_start + number_of_bins_coressponding_to_start\n","low = int(((ts_Inj-lwrlim)*60.0)/bin_width)+int((60.0/bin_width)*start)\n","#high = low + number_of_bins_corresponding_to_fit_range\n","high = low+int((60.0/bin_width)*fit_range)\n","#define x bins for fit\n","x_tmp = x_tmp[low:high+1]\n","#define y data for fit; convert x bin edges to bin centers\n","y = y[low:high]; x = x_tmp[:-1]+(np.diff(x_tmp)/2.0)\n","fit_tot = []; fit_Xe =[]; fit_Other=[]\n","\n","c_empty = np.array(y > -1.0) # Probably not necessary (was previously > 0, but testing)\n","\n","A, sig_A = fit1(x[c_empty],y[c_empty])\n","B, sig_B, tau, sig_tau = fit2(x[c_empty],y[c_empty])\n","\n","for i in x:\n","    fit_Xe.extend([A*np.exp(-i*xe_dc)])\n","    fit_Other.extend([B*np.exp(-i/tau)])\n","\n","fit_Xe = np.array(fit_Xe) \n","fit_Other = np.array(fit_Other)\n","Amp_Xe = A*np.exp(-ts_Inj*xe_dc) \n","Amp_free = B*np.exp(-ts_Inj/tau)\n","\n","plt.plot(x, fit_Xe, color='r', label=\"Xe131m Half Life Fixed\")\n","plt.plot(x, fit_Other, color='b', label = \"Half Life as Free Parameter\")\n","plt.plot([ts_Inj, ts_Inj],[0.01, 100], color='grey')\n","plt.xlabel('Minutes since '+files[0])\n","plt.xlim(ts_Inj-60,uprlim)\n","plt.ylim(min(fit_Xe)-0.2*min(fit_Xe),max(fit_Xe)+0.2*max(fit_Xe))\n","plt.ylabel('Rate [Bq]'); \n","#plt.yscale('log'); \n","plt.legend(loc='lower right')\n","plt.title(\"Single Exponential Fits, Two Different Ways\"); plt.grid(); plt.show()\n","\n","plt.figure(figsize=(13,4))\n","plt.plot(x[c_empty], y[c_empty]-fit_Xe[c_empty], color='r', label=\"Xe131m Residuals\")\n","plt.plot(x[c_empty], y[c_empty]-fit_Other[c_empty], color='b', label=\"Other Residuals\")\n","plt.plot(x, 0*x, color='k')\n","plt.xlabel('Minutes since '+files[0]); plt.ylabel('Residuals')\n","plt.xlim(ts_Inj-60,uprlim)\n","plt.legend(loc=\"lower right\"); plt.grid(); plt.show()\n","\n","tau = (tau/60.0)/24.0 # Convert tau from minutes to days\n","sig_tau = (sig_tau/60.0)/24.0\n","\n","print(\"Fixed Decay Amp = \"+str(round(Amp_Xe,2))+\" +/- \"+str(round(sig_A,2)))\n","print(\"Free Decay Amp = \"+str(round(Amp_free,2))+\" +/- \"+str(round(sig_B,2)))\n","print(\"Free Decay Tau = \"+str(round(tau,2))+\" +/- \"+str(round(sig_tau,2))+\\\n","      \" days (Half-life = \"+str(round(tau*math.log(2.0),1))+\" +/- \"+\\\n","      str(round(sig_tau*math.log(2.0),1))+\" days)\")"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"jxLj-PVbHd3C"},"source":["### Waveform Viewer ###\n","\n","# Event number: \n","events = [2]\n","\n","x1 = 10\n","x2 = 40\n","\n","plt.figure(figsize=(14,5))\n","for evt in events:\n","    plt.plot(gain_correct*waveArr[0,evt,:], label='Ch 0 (Top PMT)')\n","    plt.plot(waveArr[1,evt:], label='Ch 1 (Bottom PMT)')\n","    plt.plot(waveArrSum[evt], label='Sum')\n","plt.xlabel('Sample')\n","plt.ylabel('mV')\n","plt.title('Event '+str(evt))\n","#plt.yscale('log')\n","#plt.xlim(x1,x2)\n","plt.legend()\n","plt.grid()\n","plt.show()"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"Yildry6uHd3C"},"source":[""],"execution_count":null,"outputs":[]}]}
