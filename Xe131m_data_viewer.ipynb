rr = np.sqrt(cov[0][0]), np.sqrt(cov[1][1])\n","    return (mean, sigma, A, mean_err, sig_err)\n","\n","w1 = np.zeros(len(timestamp_min[c_good])) \n","w2 = np.zeros(len(timestamp_min[c_good & c_Xe131m])) \n","\n","w1.fill(1.0/bin_width); w2.fill(1.0/bin_width);  \n","\n","y0, x0 = np.histogram(timestamp_min[c_good], bins=binN, range=[lwrlim,uprlim], \\\n","                     weights=w1)\n","y1, x1 = np.histogram(timestamp_min[c_good & c_Xe131m], bins=binN, range=[lwrlim,uprlim], \\\n","                     weights=w2)\n","\n","x_bfor = x1[low_bfor:high_bfor+1]\n","x_bfor = x_bfor[:-1]+(np.diff(x_bfor)/2.0)\n","y_bfor = y1[low_bfor:high_bfor]\n","x_aftr = x1[low_aftr:high_aftr+1]\n","x_aftr = x_aftr[:-1]+(np.diff(x_aftr)/2.0)\n","y_aftr = y1[low_aftr:high_aftr]\n","\n","\n","f, (ax1,ax2) = plt.subplots(nrows=1, ncols=2, figsize=(12,5))\n","yb, xb, _ = ax1.hist(y_bfor, bins=40, histtype='step')\n","ya, xa, _ = ax2.hist(y_aftr, bins=40, histtype='step')\n","ax1.set_xlabel(\"Rate (Hz)\")\n","ax1.set_title(\"Before Injection\"); ax1.grid();\n","ax2.set_xlabel(\"Rate (Hz)\")\n","ax2.set_title(\"After Injection\"); ax2.grid();\n","xb = xb[:-1]+(np.diff(xb)/2.0)\n","xa = xa[:-1]+(np.diff(xa)/2.0)\n","mean_bfor, std_bfor, B, mean_bfor_err, sig_bfor_err = fit_rateB(xb, yb)\n","mean_aftr, std_aftr, A, mean_aftr_err, sig_aftr_err = fit_rateA(xa, ya)\n","ax1.plot(xb, gauss(xb, mean_bfor, std_bfor, B), color='r')\n","ax2.plot(xa, gauss(xa, mean_aftr, std_aftr, A), color='r')\n","plt.show()\n","\n","\n","print(\"Time in 'Before Fit' = \"+str(time_bfor-(mod*bin_width/60.0))+ \" min\")\n","print(\"Time in 'After Fit'  = \"+str(time_aftr)+ \" min\")\n","print(\"Mixing Time Before 'After Fit' = \"+str(mix_t)+ \" min\"); print(\" \")\n","\n","print(\"Xe131m Rate Before Injection = \"+str(round(mean_bfor,2))+\\\n","      \" +/- \"+str(round(mean_bfor_err,2))+\" Bq\")\n","print(\"Xe131m Rate After Injection  = \"+str(round(mean_aftr,2))+\\\n","      \" +/- \"+str(round(mean_aftr_err,2)